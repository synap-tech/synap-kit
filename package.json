{
  "name": "synap-test-kit",
  "description": "A comprehensive UI library for Synap-Tech's ERP products, containing common components, layouts, utils, and core features like forms and tables to streamline development and maintenance.",
  "keywords": [
    "react",
    "erp",
    "ui-kit",
    "component-library",
    "synap-tech",
    "shadcn",
    "tailwindcss",
    "react-hook-form",
    "tanstack-table",
    "tanstack-query"
  ],
  "author": "Synap-Tech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/synap-tech/synap-kit"
  },
  "private": false,
  "version": "0.0.5",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "style": "./dist/style.css",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index"
    },
    "./types": {
      "types": "./dist/types/types/index.d.ts",
      "import": "./dist/types/types/index.d"
    },
    "./providers": {
      "types": "./dist/types/providers/index.d.ts",
      "import": "./dist/providers"
    },
    "./layout": {
      "types": "./dist/types/components/layout/index.d.ts",
      "import": "./dist/layout"
    },
    "./login": {
      "types": "./dist/types/components/auth/login/index.d.ts",
      "import": "./dist/login"
    },
    "./form": {
      "types": "./dist/types/components/core/form/index.d.ts",
      "import": "./dist/form"
    },
    "./data-table": {
      "types": "./dist/types/components/core/data-table/index.d.ts",
      "import": "./dist/data-table"
    },
    "./data-table-entry": {
      "types": "./dist/types/components/core/data-table/entry.d.ts",
      "import": "./dist/data-table-entry"
    },
    "./data-table-ssr": {
      "types": "./dist/types/components/core/data-table-ssr/index.d.ts",
      "import": "./dist/data-table-ssr"
    },
    "./components/*": {
      "types": "./dist/types/components/ui/*.d.ts",
      "import": "./dist/components/ui/*.js"
    },
    "./modal/*": {
      "types": "./dist/types/components/core/modal/*.d.ts",
      "import": "./dist/components/core/modal/*.js"
    },
    "./buttons/*": {
      "types": "./dist/types/components/buttons/*.d.ts",
      "import": "./dist/components/buttons/*.js"
    },
    "./config/*": {
      "types": "./dist/types/config/*.d.ts",
      "import": "./dist/config/*.js"
    },
    "./hooks/*": {
      "types": "./dist/types/hooks/*.d.ts",
      "import": "./dist/hooks/*.js"
    },
    "./utils/*": {
      "types": "./dist/types/utils/*.d.ts",
      "import": "./dist/utils/*.js"
    },
    "./lib/*": {
      "types": "./dist/types/lib/*.d.ts",
      "import": "./dist/lib/*.js"
    },
    "./tailwind-config": {
      "types": "./dist/tailwind-config.d.ts",
      "import": "./dist/tailwind-config.js"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly --outDir dist/types",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "preview": "vite preview",
    "deploy": "git pull && npm i && npm run build && npm link"
  },
  "peerDependencies": {
    "@handsontable/react": "14.6.1",
    "@headlessui/react": "^2.2.7",
    "@hookform/devtools": "^4.4.0",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.84.2",
    "@tanstack/react-table": "^8.21.3",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "handsontable": "^14.6.1",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.536.0",
    "react": "^19.1.1",
    "react-csv": "^2.2.2",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.65.0",
    "react-hotkeys-hook": "^5.1.0",
    "react-paginate": "^8.3.0",
    "react-quill-new": "^3.4.6",
    "react-router": "^7.9.4",
    "react-select": "^5.10.2",
    "react-toastify": "^10.0.5",
    "sonner": "^2.0.7",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/match-sorter-utils": "8.19.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "motion": "^12.23.22",
    "react-aria-components": "^1.13.0",
    "react-color": "^2.19.3",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "use-mask-input": "^3.5.2",
    "use-react-router-breadcrumbs": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@faker-js/faker": "^10.1.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@tanstack/react-table": "^8.21.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.20",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-color": "^3.0.13",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "^19.1.7",
    "@uidotdev/usehooks": "^2.4.1",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "lodash-es": "^4.17.21",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-dropzone": "^14.3.8",
    "rollup-plugin-visualizer": "^6.0.3",
    "sonner": "^2.0.7",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vite-plugin-dts": "^4.5.4"
  }
}
